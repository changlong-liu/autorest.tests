{
  "name": "@autorest/tests",
  "version": "0.0.1",
  "description": "Autorest test generation extension",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "start": "node dist/src/index.js",
    "debug": "node --max_old_space_size=4096 --inspect-brk ./dist/src/index.js",
    "eslint-fix": "eslint  . --fix --ext .ts",
    "eslint": "eslint  . --ext .ts",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "test": "npm run build && mocha dist",
    "clean": "ver > nul 2>&1 nul && npm run --silent clean-cmd || npm run --silent clean-bash",
    "clean-cmd": "if exist dist rmdir /s /q dist && exit 0 || exit 0 ",
    "clean-bash": "rm nul && rm -rf dist && exit 0 || exit 0",
    "postbuild": "cpy template ../dist/src --cwd=src/ --parents"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/autorest.az.git"
  },
  "keywords": [
    "autorest",
    "extension"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "10.17.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^26.6.3",
    "node-yaml": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@autorest/go": "^4.0.0-preview.22",
    "@azure-tools/autorest-extension-base": "~3.1.0",
    "@azure-tools/codemodel": "^4.13.349",
    "cpy-cli": "^3.1.1",
    "lodash": "^4.17.21",
    "node-yaml": "^3.2.0",
    "nunjucks": "^3.2.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
